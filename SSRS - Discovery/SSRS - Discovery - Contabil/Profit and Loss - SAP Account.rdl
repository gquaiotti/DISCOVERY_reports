<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="PROTHEUS">
      <DataSourceReference>PROTHEUS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>d9e81aac-7b72-4975-b22a-de52d7d72ec1</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="profit_loss_ds">
      <Query>
        <DataSourceName>PROTHEUS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_d_saldo_anterior_de">
            <Value>=Parameters!p_d_saldo_anterior_de.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_d_data_de">
            <Value>=Parameters!p_d_data_de.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_d_data_ate">
            <Value>=Parameters!p_d_data_ate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_n_grupo">
            <Value>=Parameters!p_n_grupo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_n_moeda">
            <Value>=Parameters!p_n_moeda.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>begin

	-- Parametros do relatorio
	--declare @p_d_data_de date = '2019-11-01'
	--declare @p_d_data_ate date = '2019-12-31'
	--declare @p_d_saldo_anterior_de date = '2019-10-15'

	-- Serie para periodo
	-- Todos os meses do periodo, mesmo que nao exista saldo ou lancamento
	declare @t_perido 
	  table (id int,
	         n_ano int,
	         n_mes int,
			 d_data_de date,
			 d_data_ate date)

	-- Tabela de resultado
	declare @t_balance
	  table (id_periodo int,
			 n_ano int,
			 n_mes int,
			 d_data_de date,
			 d_data_ate date,
			 id_conta_contabil int,
			 c_conta_contabil varchar(30),
			 c_condicao varchar(1),
			 id_grupo_contabil int,
			 c_grupo_contabil varchar(8),
			 c_descricao varchar(60),
			 id_centro_custo int,
			 c_centro_custo varchar(9),
			 c_descricao_cc varchar(40),
			 n_saldo_final_anterior float,
			 n_saldo_lancto float,
			 n_saldo_final float)

	--
	declare @v_d_saldo_anterior_de date

	if datefromparts(year(@p_d_saldo_anterior_de), month(@p_d_saldo_anterior_de), 1) &lt; datefromparts(year(@p_d_data_de), month(@p_d_data_de), 1)
		set @v_d_saldo_anterior_de = datefromparts(year(@p_d_saldo_anterior_de), month(@p_d_saldo_anterior_de), 1)
	else
		set @v_d_saldo_anterior_de = datefromparts(year(@p_d_data_de), month(@p_d_data_de), 1)

	--
	-- Geracao da serie temporal
	--
	declare @v_n_id int = 1
	declare @v_n_ano int
	declare @v_n_mes int

	set @v_n_ano = year(@p_d_data_de)
	set @v_n_mes = month(@p_d_data_de)

	while (@v_n_ano &lt; year(@p_d_data_ate) or (@v_n_ano = year(@p_d_data_ate) and @v_n_mes &lt;= month(@p_d_data_ate)))
	begin
		insert into @t_perido (id, n_ano, n_mes, d_data_de) values (@v_n_id, @v_n_ano, @v_n_mes, DATEFROMPARTS(@v_n_ano, @v_n_mes, 1))

		update @t_perido
		   set d_data_ate = DATEADD(DAY, -1, DATEADD(MONTH, 1, d_data_de))

		if @v_n_mes &lt; 12
			set @v_n_mes += 1
		else
		begin
			set @v_n_mes = 1
			set @v_n_ano += 1
		end
		
		set @v_n_id += 1
	end

	-- Contempla todas as contas que tenham saldo
	insert into @t_balance 
		(id_periodo,
		 n_ano, 
		 n_mes, 
		 d_data_de,
		 d_data_ate,
		 id_conta_contabil,
		 c_conta_contabil,
		 c_condicao,
		 id_grupo_contabil,
		 c_grupo_contabil, 
		 c_descricao,
		 id_centro_custo,
		 c_centro_custo,
		 c_descricao_cc)
	select p.id as id_periodo,
		   p.n_ano as n_ano,
	       p.n_mes as n_mes,
		   p.d_data_de,
		   p.d_data_ate,
		   l.id_conta_contabil,
		   l.c_conta_contabil as c_conta_contabil,
		   left(cc.c_condicao, 1) as c_condicao,
		   cc.id_grupo_contabil,
		   cc.c_grupo as c_grupo_contabil,
		   g.c_descricao as c_descricao,
		   -1 as id_centro_custo,
		   null as c_centro_custo,
		   null as c_descricao_cc
	  from bi_ctb_saldo_conta_mes_vw l
	  join bi_ctb_conta_contabil_vw cc
	    on cc.id = l.id_conta_contabil
	  join bi_ctb_grupo_contabil_vw g
	    on g.id = cc.id_grupo_contabil
	  join @t_perido p
	    on 1=1
	 where not (LEFT(l.c_conta_contabil, 1) = '1' OR LEFT(l.c_conta_contabil, 1) = '2')
	   and cc.id_grupo_contabil in (@p_n_grupo)
	   and l.id_moeda in (@p_n_moeda)
	 group by  p.id,
	           p.n_ano,
			   p.n_mes,
			   p.d_data_de,
			   p.d_data_ate,
			   l.id_conta_contabil,
			   l.c_conta_contabil,
			   left(cc.c_condicao, 1),
			   cc.id_grupo_contabil,
			   cc.c_grupo,
			   g.c_descricao
     union
	select p.id as id_periodo,
		   p.n_ano as n_ano,
	       p.n_mes as n_mes,
		   p.d_data_de,
		   p.d_data_ate,
		   l.id_conta_contabil,
		   l.c_conta_contabil as c_conta_contabil,
		   left(cc.c_condicao, 1) as c_condicao,
		   cc.id_grupo_contabil,
		   cc.c_grupo as c_grupo_contabil,
		   g.c_descricao as c_descricao,
		   ce.id as id_centro_custo,
		   ce.c_centro_custo as c_centro_custo,
		   ce.c_descricao as c_descricao_cc
	  from bi_ctb_saldo_cc_mes_vw l
	  join bi_ctb_conta_contabil_vw cc
	    on cc.id = l.id_conta_contabil
	  join bi_ctb_grupo_contabil_vw g
	    on g.id = cc.id_grupo_contabil
	  join bi_ctb_centro_custo_vw ce
	    on ce.id = l.id_centro_custo
	  join @t_perido p
	    on 1=1
	 where not (LEFT(l.c_conta_contabil, 1) = '1' OR LEFT(l.c_conta_contabil, 1) = '2')
	   and cc.id_grupo_contabil in (@p_n_grupo)
	   and l.id_moeda in (@p_n_moeda)
	 group by  p.id,
	           p.n_ano,
			   p.n_mes,
			   p.d_data_de,
			   p.d_data_ate,
			   l.id_conta_contabil,
			   l.c_conta_contabil,
			   left(cc.c_condicao, 1),
			   cc.id_grupo_contabil,
			   cc.c_grupo,
			   g.c_descricao,
			   ce.id,
			   ce.c_centro_custo,
			   ce.c_descricao
	order by c_grupo,
	         c_conta_contabil,
	         n_ano, 
	         n_mes

	-- Ultimo saldo anterior disponivel na base antes da data inicial
	-- Somente para o primeiro periodo
	UPDATE s
	   SET n_saldo_final_anterior = (SELECT SUM(sc.n_saldo)
										FROM bi_ctb_saldo_cc_mes_vw sc
									   WHERE sc.c_conta_contabil = s.c_conta_contabil
									     AND sc.c_centro_custo = s.c_centro_custo
										 AND sc.d_data &lt; s.d_data_de
										 AND sc.d_data &gt;= @v_d_saldo_anterior_de
	                                     AND sc.id_moeda in (@p_n_moeda))
      FROM @t_balance s
	 WHERE s.id_periodo = 1

	-- Saldo final calculado
	UPDATE s
	   SET n_saldo_lancto = (SELECT SUM(sc.n_saldo)
								FROM bi_ctb_saldo_cc_mes_vw sc
							   WHERE sc.c_conta_contabil = s.c_conta_contabil
								 AND sc.c_centro_custo = s.c_centro_custo
								 AND sc.d_data between s.d_data_de and s.d_data_ate
	                             AND sc.id_moeda in (@p_n_moeda))
      FROM @t_balance s

	-- Saldo final calculado
	UPDATE s
	   SET n_saldo_final = (SELECT SUM(sc.n_saldo)
						      FROM bi_ctb_saldo_cc_mes_vw sc
							 WHERE sc.c_conta_contabil = s.c_conta_contabil
							   AND sc.c_centro_custo = s.c_centro_custo
							   AND sc.d_data &lt;= s.d_data_ate
							   AND sc.d_data &gt;= @v_d_saldo_anterior_de
	                           AND sc.id_moeda in (@p_n_moeda))
      FROM @t_balance s

	-- Tratamento para gerar linha com centro de custos em branco
	-- quando houver diferenca entre o saldo da conta e a soma do saldo por CC
	-- Ultimo saldo anterior disponivel na base antes da data inicial
	-- Somente para o primeiro periodo
	UPDATE s
	   SET n_saldo_final_anterior = ISNULL((SELECT SUM(sc.n_saldo)
											  FROM bi_ctb_saldo_conta_mes_vw sc
											 WHERE sc.c_conta_contabil = s.c_conta_contabil
											   AND sc.d_data &lt; s.d_data_de
											   AND sc.d_data &gt;= @v_d_saldo_anterior_de
	                                           AND sc.id_moeda in (@p_n_moeda)), 0)
								  - ISNULL((SELECT SUM(sc.n_saldo)
										      FROM bi_ctb_saldo_cc_mes_vw sc
									         WHERE sc.c_conta_contabil = s.c_conta_contabil
										       AND sc.d_data &lt; s.d_data_de
										       AND sc.d_data &gt;= @v_d_saldo_anterior_de
	                                           AND sc.id_moeda in (@p_n_moeda)), 0)
      FROM @t_balance s
	 WHERE s.id_periodo = 1
	   AND s.c_centro_custo is null

	-- Saldo final calculado
	UPDATE s
	   SET n_saldo_lancto = ISNULL((SELECT SUM(sc.n_saldo)
									  FROM bi_ctb_saldo_conta_mes_vw sc
									 WHERE sc.c_conta_contabil = s.c_conta_contabil
									   AND sc.d_data between s.d_data_de and s.d_data_ate
	                                   AND sc.id_moeda in (@p_n_moeda)), 0)
						  - ISNULL((SELECT SUM(sc.n_saldo)
								      FROM bi_ctb_saldo_cc_mes_vw sc
								     WHERE sc.c_conta_contabil = s.c_conta_contabil
									   AND sc.d_data between s.d_data_de and s.d_data_ate
	                                   AND sc.id_moeda in (@p_n_moeda)), 0)
      FROM @t_balance s
	 WHERE s.c_centro_custo is null

	-- Saldo final calculado
	UPDATE s
	   SET n_saldo_final = ISNULL((SELECT SUM(sc.n_saldo)
									 FROM bi_ctb_saldo_conta_mes_vw sc
									WHERE sc.c_conta_contabil = s.c_conta_contabil
									  AND sc.d_data &lt;= s.d_data_ate
									  AND sc.d_data &gt;= @v_d_saldo_anterior_de
	                                  AND sc.id_moeda in (@p_n_moeda)), 0)
						 - ISNULL((SELECT SUM(sc.n_saldo)
								     FROM bi_ctb_saldo_cc_mes_vw sc
								    WHERE sc.c_conta_contabil = s.c_conta_contabil
									  AND sc.d_data &lt;= s.d_data_ate
									  AND sc.d_data &gt;= @v_d_saldo_anterior_de
	                                  AND sc.id_moeda in (@p_n_moeda)), 0)
      FROM @t_balance s
	 WHERE s.c_centro_custo is null

	SELECT n_ano, 
	       n_mes, 
		   d_data_de,
		   d_data_ate,
		   id_grupo_contabil,
		   c_grupo_contabil, 
		   c_descricao, 
		   id_centro_custo,
		   c_centro_custo,
		   c_descricao_cc,
		   SUM(n_saldo_final_anterior) as n_saldo_final_anterior,
		   SUM(n_saldo_lancto) as n_saldo_lancto,
		   SUM(n_saldo_final) as n_saldo_final
	  FROM @t_balance
	 WHERE NOT (n_saldo_final_anterior = 0 AND n_saldo_lancto = 0 AND n_saldo_final = 0)
	 GROUP BY n_ano, 
	          n_mes, 
			  d_data_de,
			  d_data_ate,
			  id_grupo_contabil,
		      c_grupo_contabil, 
		      c_descricao,
			  id_centro_custo,
			  c_centro_custo,
			  c_descricao_cc
	 ORDER BY c_grupo_contabil, 
	 		  c_centro_custo,
			  n_ano, 
	          n_mes
end</CommandText>
      </Query>
      <Fields>
        <Field Name="n_ano">
          <DataField>n_ano</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="n_mes">
          <DataField>n_mes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="d_data_de">
          <DataField>d_data_de</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="id_grupo_contabil">
          <DataField>id_grupo_contabil</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="d_data_ate">
          <DataField>d_data_ate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="c_grupo_contabil">
          <DataField>c_grupo_contabil</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="c_descricao">
          <DataField>c_descricao</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="c_centro_custo">
          <DataField>c_centro_custo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="id_centro_custo">
          <DataField>id_centro_custo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="n_saldo_final_anterior">
          <DataField>n_saldo_final_anterior</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="c_descricao_cc">
          <DataField>c_descricao_cc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="n_saldo_lancto">
          <DataField>n_saldo_lancto</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="n_saldo_final">
          <DataField>n_saldo_final</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
